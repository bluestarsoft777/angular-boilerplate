{
    "env": {
        "browser": true
    },
    "ecmaFeatures": {
        "blockBindings": true,
        "forOf": true
    },
    "rules": {
        "semi": 4,
        "quotes": [1, "single"],
        "strict": 0,
        "no-trailing-spaces": [0],
        "no-unused-vars": [1],
        "global-strict": true,
        "no-console": false
    },
    "globals": {
      "require": true
    }
}
